<%- include('partials/header') %>

<table class="table table-bordered table-hover">
    <thead class="thead-dark">
        <tr>
            <td>S.No</td>
            <td>ISBN</td>
            <td>Title</td>
            <td>Hold Date</td>
        </tr>
        <% for(var i=1;i<=results.length;i++){   %>
            <tr>
                <td><%= i %></td>
                <td><%= results[i-1].ISBN %></td>
                <td><%= results[i-1].title %></td>
                <td><%= (results[i-1].Date_of_hold).toISOString().substring(0,10) %></td>
            </tr>
        <% }    %>
    </thead>
</table>

<%- include('partials/footer') %>